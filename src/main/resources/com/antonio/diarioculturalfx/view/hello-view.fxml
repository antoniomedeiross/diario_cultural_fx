      spacing="20"
      prefWidth="1000"
      prefHeight="800">

    <VBox alignment="CENTER"
        maxWidth="1080"
        minWidth="500"
          minHeight="500"
        fx:id="div_titulo"
        spacing="20">

        <Label text="DIÁRIO CULTURAL" fx:id="titulo"/>
        <Label text="Bem-vindo"/>
        <Button text="Entrar" fx:id="botaoEntrar"/>
    </VBox>
</VBox>
